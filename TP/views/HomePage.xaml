<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TP.views.HomePage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="home page"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="go to MasterDetailPage" Clicked="MasterDetailPage"/>
            <Button Text="go to TabbedPage" Clicked="TabbedPage"/>
            <Button Text="go to CarouselPage" Clicked="CarouselPage"/>
            <Button Text="go to NavigationPage" Clicked="NavigationPage"/>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>