<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TP.Views.TabbedPageD"
            Title="TabbedPage">
    <TabbedPage.Children>
        <ContentPage Title="page1" BackgroundColor="DarkCyan">
            <Label Text="page 1" VerticalOptions="Center"  HorizontalOptions="Center" FontSize="22"/>
        </ContentPage>

        <ContentPage Title="page2" BackgroundColor="Silver">
            <Label Text="page 2" VerticalOptions="Center"  HorizontalOptions="Center" FontSize="22"/>
        </ContentPage>


        <ContentPage Title="page3" BackgroundColor="Crimson">
            <Label Text="page 3" VerticalOptions="Center"  HorizontalOptions="Center" FontSize="22"/>
        </ContentPage>


    </TabbedPage.Children>
</TabbedPage>